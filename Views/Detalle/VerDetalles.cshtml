@model IEnumerable<WebAppProyectoFinal.Models.TbDetalleReserva>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<html>
    <head>

    </head>
    <body>
        <h3 class="alert-primary">Listado de detalles</h3>
        <table class="table table-bordered">
            <tr>
                <td>IdReserva</td>
                <td>IdLibro</td>
                <td>Fecha Reserva</td>
                <td>Fecha Devolución</td>
                <td>Cantidad</td>
            </tr>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.IdPrestamo</td>
                        <td>@item.IdLibro</td>
                        <td>@item.FecPrestamo</td>
                        <td>@item.FecDevolucion</td>
                        <td>@item.Cantidad</td>
                    </tr>
                }
            </tbody>
        </table>
    </body>
</html>